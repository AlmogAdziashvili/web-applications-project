{
  "name": "web-application-project",
  "version": "1.0.0",
  "main": "server/index.js",
  "scripts": {
    "build": "npm run build:prd",
    "build:prd": "NODE_ENV=production webpack --mode=production --config ./client/config/webpack.prd.config.js",
    "build:dev": "NODE_ENV=development webpack --mode=development --config ./client/config/webpack.dev.config.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "nodemon server/index.js",
    "start:client": "NODE_ENV=development webpack --mode=development --config ./client/config/webpack.dev.config.js --watch"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "express": "^4.18.2",
    "mongoose": "^7.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3"
  }
}
